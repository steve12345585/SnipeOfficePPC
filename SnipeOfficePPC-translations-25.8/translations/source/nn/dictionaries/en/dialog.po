#: en_en_US.properties#spelling.property.text
msgid "Grammar checking"
msgstr "Grammatikkkontroll"

#: en_en_US.properties#hlp_grammar.property.text
msgid "Check more grammar errors."
msgstr "Kontroller fleire grammatikkfeil."

#: en_en_US.properties#grammar.property.text
msgid "Possible mistakes"
msgstr "Moglege feil"

#: en_en_US.properties#hlp_cap.property.text
msgid "Check missing capitalization of sentences."
msgstr "Kontroller om manglande stor forbokstav i setningar."

#: en_en_US.properties#cap.property.text
msgid "Capitalization"
msgstr "Stor forbokstav"

#: en_en_US.properties#hlp_dup.property.text
msgid "Check repeated words."
msgstr "Sjå etter gjentatte ord"

#: en_en_US.properties#dup.property.text
msgid "Word duplication"
msgstr "Kloning av ord"

#: en_en_US.properties#hlp_pair.property.text
msgid "Check missing or extra parentheses and quotation marks."
msgstr "Sjå etter manglande eller ekstra parentesar og hermeteikn."

#: en_en_US.properties#pair.property.text
msgid "Parentheses"
msgstr "Parentesar"

#: en_en_US.properties#punctuation.property.text
msgid "Punctuation"
msgstr "Teiknsetjing"

#: en_en_US.properties#hlp_spaces.property.text
msgid "Check single spaces between words."
msgstr "Kontroller enkle mellomrom mellom ord."

#: en_en_US.properties#spaces.property.text
msgid "Word spacing"
msgstr "Ordavstand"

#: en_en_US.properties#hlp_mdash.property.text
msgid "Force unspaced em dash instead of spaced en dash."
msgstr "Bruk lang tankestrek utan mellomrom i staden for kort tankestrek med mellomrom."

#: en_en_US.properties#mdash.property.text
msgid "Em dash"
msgstr "Lang tankestrek"

#: en_en_US.properties#hlp_ndash.property.text
msgid "Force spaced en dash instead of unspaced em dash."
msgstr "Bruk kort tankestrek med mellomrom i staden for lang tankestrek utan mellomrom."

#: en_en_US.properties#ndash.property.text
msgid "En dash"
msgstr "Kort tankestrek"

#: en_en_US.properties#hlp_quotation.property.text
msgid "Check double quotation marks: \"x\" → “x”"
msgstr "Kontroller doble hermeteikn: «x» → «y»"

#: en_en_US.properties#quotation.property.text
msgid "Quotation marks"
msgstr "Hermeteikn"

#: en_en_US.properties#hlp_times.property.text
msgid "Check true multiplication sign: 5x5 → 5×5"
msgstr "Ekte multiplikasjonsteikn: 5 x 5 → 5 × 5"

#: en_en_US.properties#times.property.text
msgid "Multiplication sign"
msgstr "Multiplikasjonsteikn"

#: en_en_US.properties#hlp_spaces2.property.text
msgid "Check single spaces between sentences."
msgstr "Kontroller enkle mellomrom mellom setningar."

#: en_en_US.properties#spaces2.property.text
msgid "Sentence spacing"
msgstr "Mellomrom i setningar"

#: en_en_US.properties#hlp_spaces3.property.text
msgid "Check more than two extra space characters between words and sentences."
msgstr "Kontroller om meir enn to ekstra mellomromstegn mellom ord og setningar."

#: en_en_US.properties#spaces3.property.text
msgid "More spaces"
msgstr "Fleire mellomrom"

#: en_en_US.properties#hlp_minus.property.text
msgid "Change hyphen characters to real minus signs."
msgstr "Endre bindestreker til ekte minusteikn."

#: en_en_US.properties#minus.property.text
msgid "Minus sign"
msgstr "Minusteikn"

#: en_en_US.properties#hlp_apostrophe.property.text
msgid "Change typewriter apostrophe, single quotation marks and correct double primes."
msgstr "Endre skrivemaskinapostrof, enkle hermeteikn og rett opp doble strekteikn."

#: en_en_US.properties#apostrophe.property.text
msgid "Apostrophe"
msgstr "Apostrof"

#: en_en_US.properties#hlp_ellipsis.property.text
msgid "Change three dots with ellipsis."
msgstr "Endre tre punktum til ellipseteikn."

#: en_en_US.properties#ellipsis.property.text
msgid "Ellipsis"
msgstr "Ellipseteikn"

#: en_en_US.properties#others.property.text
msgid "Others"
msgstr "Andre"

#: en_en_US.properties#hlp_metric.property.text
msgid "Measurement conversion from °F, mph, ft, in, lb, gal and miles."
msgstr "Gjer om måleeiningar frå °F, mph, ft, in, lb, gal og miles."

#: en_en_US.properties#metric.property.text
msgid "Convert to metric (°C, km/h, m, kg, l)"
msgstr "Gjer om til metrisk (°C, km/h, m, kg, l)"

#: en_en_US.properties#hlp_numsep.property.text
msgid "Common (1000000 → 1,000,000) or ISO (1000000 → 1 000 000)."
msgstr "Vanleg (1000000 → 1,000,000) eller ISO (1000000 → 1 000 000)."

#: en_en_US.properties#numsep.property.text
msgid "Thousand separation of large numbers"
msgstr "Tusenskille for store tal"

#: en_en_US.properties#hlp_nonmetric.property.text
msgid "Measurement conversion from °C; km/h; cm, m, km; kg; l."
msgstr "Gjer om måleeiningar frå °C, km/h, cm, m, km, kg, l."

#: en_en_US.properties#nonmetric.property.text
msgid "Convert to non-metric (°F, mph, ft, lb, gal)"
msgstr "Gjer om til ikkje-metrisk (°F, mph, ft, lb, gal)"

#: OptionsDialog.xcu#..OptionsDialog.Nodes.org.openoffice.lightproof.Label.value.text
msgid "Dictionaries"
msgstr "Ordlister"

#: OptionsDialog.xcu#..OptionsDialog.Nodes.org.openoffice.lightproof.Leaves.org.openoffice.lightproof.en.Label.value.text
msgid "English sentence checking"
msgstr "Engelsk setningskontroll"
