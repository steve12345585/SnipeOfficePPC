    FixedText FT_GRAPHICOBJECTCACHE_UNIT
    {
        Pos = MAP_APPFONT( COL5, OFFS_TEXTBOX_FIXEDTEXT( ROW4 ) );
        Size = MAP_APPFONT( WIDTH3, RSC_CD_FIXEDTEXT_HEIGHT );
        Text [ en-US ] = "MB";
    };
