        Text [ en-US ] = "~Protected" ;
        TabStop = TRUE ;
    };
    TriStateBox BTN_HIDE_FORMULAR
    {
        HelpID = "sc:TriStateBox:RID_SCPAGE_PROTECTION:BTN_HIDE_FORMULAR";
        Pos = MAP_APPFONT ( 22 , 42 ) ;
