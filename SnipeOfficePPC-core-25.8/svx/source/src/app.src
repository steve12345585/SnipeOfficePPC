    };

ImageList RID_DEFAULTIMAGELIST_SC
{
    Prefix = "sc";
    MASKCOLOR
    IMAGE_ID_LIST
};

ImageList RID_DEFAULTIMAGELIST_LC
{
    Prefix = "lc";
    MASKCOLOR
    IMAGE_ID_LIST
};

