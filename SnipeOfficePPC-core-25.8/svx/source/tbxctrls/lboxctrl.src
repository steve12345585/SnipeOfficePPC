    HelpID = HID_SVXTBX_UNDO_REDO_CTRL ;
    StdPopup = TRUE;
    Hide = TRUE ;
    OutputSize = TRUE ;

    ListBox LB_SVXTBX_UNDO_REDO_CTRL
